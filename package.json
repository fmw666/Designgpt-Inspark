    "@heroicons/react": "^2.1.1",
    "firebase": "^10.8.0",
    "framer-motion": "^12.7.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
